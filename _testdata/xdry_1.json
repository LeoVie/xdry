{
  "settings": {
    "minCloneLengths": {
      "level-1": 10,
      "level-2": 20
    },
    "logPath": "%pwd%/test/_testdata/xdry.log"
  },
  "directories": [
    "%pwd%/test/_testdata/php",
    "./test/_testdata/javascript"
  ],
  "normalizers": [
    {
      "level": 1,
      "extension": ".php",
      "language": "php",
      "command": "php",
      "args": [
        "%FILEPATH%"
      ]
    }
  ]
}